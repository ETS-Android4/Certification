<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.certification">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="locked"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".BookmarkActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".CalendarActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".CertificationActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".DetailcertifiActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".DetailjobActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".JobActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".MaincertifiActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".MainjobActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="locked"
            android:theme="@style/splash_background">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TransparentActivity"
            android:screenOrientation="locked"
            android:theme="@style/Transparent"></activity>
    </application>

</manifest>